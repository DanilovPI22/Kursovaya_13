
public class Mathematics  {
	
	protected static double result;

	
	public static double Math (){
		
		Abstract lolo = new Extender();
		
 
	 
		int i = MathRgr.reklama.getSelectedIndex();// Получение значения changeBox
		switch(i) {
		case 0: {
			MathRgr.n = MathRgr.text1.getText().length();			
			MathRgr.h = Extender.Parse(MathRgr.text7.getText());    	    	  	 
		    break;
		}
		case 1: {			
			MathRgr.h = Extender.Parse(MathRgr.text.getText()); // Высота			
			MathRgr.w = Extender.Parse(MathRgr.text2.getText()); //Ширина		 		 
			    break;
		}
		case 2: {
			MathRgr.h = Extender.Parse(MathRgr.text3.getText());
			MathRgr.w = Extender.Parse(MathRgr.text4.getText()); //Ширина 			 
			    break;
		}
		}
		
		 
	   
		 result = (MathRgr.T * MathRgr.h * MathRgr.w * MathRgr.k * MathRgr.n * MathRgr.f * MathRgr.j * MathRgr.c * MathRgr.a) - (MathRgr.T * MathRgr.h * MathRgr.w * MathRgr.k * MathRgr.n * MathRgr.f * MathRgr.j * MathRgr.c * MathRgr.a * lolo.abs());
		
		result = Math.round(result);
		
		return result;
	}
	
	
	
	
}
